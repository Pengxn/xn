{
    "version": "1.12.8",
    "homepage": "https://golang.org",
    "description": "Go is an open source programming language.",
    "license": "BSD-3-Clause",
    "extract_dir": "go",
    "persist": [
        ".go"
    ],
    "env_set": {
        "GOROOT": "$dir",
        "GOPATH": "$dir\\.go",
        "GOPROXY": "https://goproxy.io"
    },
    "env_add_path": [
        "bin",
        ".go\\bin"
    ],
    "architecture": {
        "64bit": {
            "url": "https://dl.google.com/go/go1.12.8.windows-amd64.zip",
            "hash": "4352ed90240ddc1b6379adf3210b849b8e89a173ca00616f2beff53df9fef3c8"
        },
        "32bit": {
            "url": "https://dl.google.com/go/go1.12.8.windows-386.zip",
            "hash": "e07fec097549c4bba0cf5712723f459c56c812f29056707c16581684e45485f2"
        }
    },
    "checkver": {
        "url": "https://golang.google.cn/dl/",
        "regex": "go([\\d.]+)\\."
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://dl.google.com/go/go$version.windows-amd64.zip"
            },
            "32bit": {
                "url": "https://dl.google.com/go/go$version.windows-386.zip"
            }
        },
        "hash": {
            "url": "$url.sha256"
        }
    },
    "notes": [
        "Your GOROOT has been set to: $dir,",
        "and GOPATH has been set to: $dir\\.go,",
        "and GOPROXY has been set to: https://goproxy.io.",
        "You can run 'go env' to view these at any time.",
        "\"$dir\\bin\" and \"$dir\\.go\\bin\" has been added to your PATH."
    ]
}
