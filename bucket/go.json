{
    "version": "1.26.0",
    "homepage": "https://golang.org",
    "description": "Go is an open source programming language.",
    "license": "BSD-3-Clause",
    "extract_dir": "go",
    "persist": ".go",
    "env_set": {
        "GOROOT": "$dir",
        "GOPATH": "$dir\\.go",
        "GOPROXY": "https://goproxy.cn,direct"
    },
    "env_add_path": [
        "bin",
        ".go\\bin"
    ],
    "architecture": {
        "64bit": {
            "url": "https://dl.google.com/go/go1.26.0.windows-amd64.zip",
            "hash": "9bbe0fc64236b2b51f6255c05c4232532b8ecc0e6d2e00950bd3021d8a4d07d4"
        }
    },
    "checkver": {
        "url": "https://golang.google.cn/dl/",
        "regex": "go([\\d.]+)\\.windows-"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://dl.google.com/go/go$version.windows-amd64.zip"
            }
        },
        "hash": {
            "url": "$url.sha256"
        }
    },
    "notes": [
        "Your GOROOT has been set to: $dir,",
        "and GOPATH has been set to: $dir\\.go,",
        "and GOPROXY has been set to: https://goproxy.cn.",
        "You can run 'go env' to view these at any time.",
        "\"$dir\\bin\" and \"$dir\\.go\\bin\" has been added to your PATH."
    ]
}
