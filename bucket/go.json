{
    "version": "1.12.7",
    "homepage": "https://golang.org",
    "description": "Go is an open source programming language.",
    "license": "BSD-3-Clause",
    "extract_dir": "go",
    "persist": [
        ".go"
    ],
    "env_set": {
        "GOROOT": "$dir",
        "GOPATH": "$dir\\.go",
        "GOPROXY": "https://goproxy.io"
    },
    "env_add_path": [
        "bin",
        ".go\\bin"
    ],
    "architecture": {
        "64bit": {
            "url": "https://dl.google.com/go/go1.12.7.windows-amd64.zip",
            "hash": "502712c0e29edc6b9cda6fa5e4b6ff9b36e27d225373baead8708c9634aa8e50"
        },
        "32bit": {
            "url": "https://dl.google.com/go/go1.12.7.windows-386.zip",
            "hash": "bf55b97f5fd9f2fd9e4f0b182e296ee531262d1f8458169f8b42d989c2370e5c"
        }
    },
    "checkver": {
        "url": "https://golang.google.cn/dl/",
        "regex": "go([\\d.]+)\\."
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://dl.google.com/go/go$version.windows-amd64.zip"
            },
            "32bit": {
                "url": "https://dl.google.com/go/go$version.windows-386.zip"
            }
        },
        "hash": {
            "url": "$url.sha256"
        }
    },
    "notes": [
        "Your GOROOT has been set to: $dir,",
        "and GOPATH has been set to: $dir\\.go,",
        "and GOPROXY has been set to: https://goproxy.io.",
        "You can run 'go env' to view these at any time.",
        "\"$dir\\bin\" and \"$dir\\.go\\bin\" has been added to your PATH."
    ]
}
